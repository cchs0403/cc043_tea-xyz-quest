{
  "name": "@finalytic/utils",
  "version": "4.7.59",
  "publishConfig": {
    "access": "public"
  },
  "files": ["build/main", "build/module", "src", "!**/*.spec.*", "LICENSE"],
  "description": "Finalytic utils",
  "main": "build/main/index.js",
  "typings": "build/main/index.d.ts",
  "module": "build/module/index.js",
  "repository": "https://github.com/finalytic/utils",
  "license": "UNLICENSED",
  "scripts": {
    "--- Scripts: Publish ---": "",
    "pub": "npm run generate && npm run build && npm version patch && npm publish",
    "build": "rm -rf build/ && run-p build:*",
    "build:main": "tsc -b tsconfig.main.json",
    "build:module": "tsc -b tsconfig.esm.json",
    "--- Scripts: Maintenance ---": "",
    "test": "bun test",
    "up": "bunx npm-check-updates -i",
    "typecheck": "tsc",
    "lint": "biome check .",
    "lint:fix": "biome check --apply-unsafe --skip-errors .",
    "--- Scripts: NPM Hooks ---": "",
    "prepare": "husky install",
    "pre-commit": "echo precommit",
    "pre-push": "npm run typecheck && npm run lint && bun i",
    "post-merge": "bun i"
  },
  "dependencies": {
    "@types/retry": "^0.12.5",
    "blakejs": "^1.2.1",
    "currency-formatter": "^1.5.9",
    "currency.js": "^2.0.4",
    "dayjs": "^1.11.10",
    "eventemitter3": "^5.0.1",
    "fast-safe-stringify": "^2.1.1",
    "parse-duration": "^1.1.0",
    "retry": "^0.13.1",
    "serialize-error": "8.1.0",
    "ts-toolbelt": "^9.6.0",
    "tweetnacl": "^1.0.3",
    "tweetnacl-util": "^0.15.1"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "husky": "^9.0.11",
    "bun-types": "^1.0.36",
    "@types/currency-formatter": "^1.5.4",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3",
    "@biomejs/biome": "^1.6.3"
  }
}
